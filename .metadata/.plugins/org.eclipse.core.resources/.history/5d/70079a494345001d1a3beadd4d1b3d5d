package Introduction;

public class BasicPrograms {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String name="mubeena";
		String rev="";
		char[] ch=name.toCharArray();
		for(int i=ch.length-1;i>=0;i--)
		{
			rev+=ch[i];
			
		}

	}
	public static void ReverseString()
	{
		String name="mubeena";
		String rev="";
		char[] ch=name.toCharArray();
		for(int i=ch.length-1;i>=0;i--)
		{
			rev+=ch[i];
			
		}
	}

}
